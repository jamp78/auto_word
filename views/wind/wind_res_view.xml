<?xml version="1.0"?>
<odoo>
    <record id="auto_word_wind_res_list" model="ir.ui.view">
        <field name="name">Wind_Information Form</field>
        <field name="model">auto_word_wind.res</field>
        <field name="arch" type="xml">
            <tree>
                <field name="tur_id"/>
                <field name="X"/>
                <field name="Y"/>
                <field name="Z"/>
                <field name="H"/>
                <field name="Latitude"/>
                <field name="Longitude"/>
                <field name="WeibullA"/>
                <field name="WeibullK"/>
                <field name="EnergyDensity"/>
                <field name="PowerGeneration"/>
                <field name="PowerGeneration_Weak"/>
                <field name="AverageWindSpeed"/>
                <field name="AverageWindSpeed_Weak"/>
                <field name="Weak"/>
                <field name="WindShear_Avg"/>
                <field name="InflowAngle_Max"/>
                <field name="InflowAngle_Max_Deg"/>
                <field name="NextTur"/>
                <field name="NextLength_M"/>
                <field name="Sectors"/>
            </tree>
        </field>
    </record>

    <record id="auto_word_wind_res_form" model="ir.ui.view">
        <field name="name">Wind reslut Form</field>
        <field name="model">auto_word_wind.res</field>
        <field name="arch" type="xml">
            <form>
                <group string="机位结果">
                    <field name="project_id"/>
                    <field name="case_name"/>
                    <field name="Turbine"/>
                    <field name="tur_id"/>
                    <field name="X"/>
                    <field name="Y"/>
                    <field name="Z"/>
                    <field name="H"/>
                </group>
                <group string="机位结果">
                    <field name="Latitude"/>
                    <field name="Longitude"/>
                    <field name="WeibullA"/>
                    <field name="WeibullK"/>
                    <field name="EnergyDensity"/>
                    <field name="PowerGeneration"/>
                    <field name="PowerGeneration_Weak"/>
                    <field name="AverageWindSpeed"/>
                    <field name="AverageWindSpeed_Weak"/>
                    <field name="Weak"/>
                </group>
                <group string="机位结果">
                    <field name="WindShear_Avg"/>
                    <field name="InflowAngle_Max"/>
                    <field name="InflowAngle_Max_Deg"/>
                    <field name="NextTur"/>
                    <field name="NextLength_M"/>
                    <field name="Sectors"/>
                </group>
            </form>
        </field>
    </record>
</odoo>

