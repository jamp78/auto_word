<?xml version="1.0"?>
<odoo>
    <record id="auto_word_wind_turbines_compare_form" model="ir.ui.view">
        <field name="name">机型比选表</field>
        <field name="model">auto_word_wind_turbines.compare</field>
        <field name="arch" type="xml">
            <form string="机型比选表">
                <sheet>
                    <group name="group_top">
                         <group name="group_left">
                            <field name="name_tur" />
                            <field name="auto_word_wind.turbine_numbers" />
                            <field name="capacity" />
                            <field name="installed_capacity" />
                            <field name="rotor_diameter" />
                            <field name="hub_height" />
                             <field name="power_generation" />
                             <field name="weak" />
                             <field name="power_hours" />
                         </group>
                         <group name="group_right">
                            <field name="tower_weight" />
                            <field name="investment_turbines_kw" />
                            <field name="investment_e1" />
                            <field name="cut_in_wind_speed" />
                            <field name="cut_out_wind_speed" />
                             <field name="three_second_maximum" />
                        </group>
                    </group>
                    <!--<group name="group_bottom">-->
                        <!--<field name="generator_Output" />-->
                    <!--</group>-->
                </sheet>
            </form>
        </field>
    </record>

